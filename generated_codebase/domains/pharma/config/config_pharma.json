{
  "domain": {
    "name": "pharma",
    "version": "1.0.0"
  },
  "agents": {
    "default": {
      "max_retries": 3,
      "timeout_seconds": 30,
      "batch_size": 100,
      "cache_ttl_seconds": 300,
      "confidence_threshold": 0.85,
      "quality_threshold": 0.9
    }
  },
  "services": {
    "api": {
      "host": "0.0.0.0",
      "port": 8017,
      "workers": 4
    }
  },
  "database": {
    "type": "postgresql",
    "host": "localhost",
    "port": 5432,
    "name": "pharma_db"
  },
  "cache": {
    "type": "redis",
    "host": "localhost",
    "port": 6379,
    "db": 17
  }
}